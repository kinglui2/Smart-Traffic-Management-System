{
    "include": [
        "src",
        "tests"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "build",
        "dist"
    ],
    "ignore": [],
    "defineConstant": {
        "DEBUG": true
    },
    "venvPath": ".",
    "venv": "venv",
    "reportMissingImports": "none",
    "reportMissingModuleSource": "none",
    "pythonVersion": "3.10",
    "pythonPlatform": "Linux",
    "executionEnvironments": [
        {
            "root": "src",
            "extraPaths": [
                "src"
            ]
        },
        {
            "root": "tests",
            "extraPaths": [
                "src"
            ]
        }
    ]
} 